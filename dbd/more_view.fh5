  <script type="text/javascript">
  var theRequest ={};
  function GetRequest() {
    var url = location.search; //获取url中"?"符后的字串
    var a=decodeURI(url);
    if (a.indexOf("?") != -1) {
        var str = a.substr(1);
        strs = str.split("&");
        for(var i = 0; i < strs.length; i ++) {
            theRequest[strs[i].split("=")[0]]=unescape(strs[i].split("=")[1]);
        }
    }

     key=theRequest.key;
     dtitle=theRequest.dtitle;
     mh=theRequest.mh;
    return theRequest;
  }
  GetRequest();
	if(mh!='portal2'){
		loadScript('../portal/'+mh+'/e_'+mh+'.js');
	}else{
		if(window.sessionStorage.getItem('plan') != null){
			var n=window.sessionStorage.getItem('plan');
			loadScript('../portal/'+mh+'/e_'+mh+'_'+n+'.js');
		}else{
			loadScript('../portal/'+mh+'/e_'+mh+'_1.js');
		}
	}
  function loadScript(url) {
    //console.info(url);
      var script = document.createElement("script");
      script.type = "text/javascript";
      script.src = url;
      document.getElementsByTagName("head")[0].appendChild(script);
  }
</script>
