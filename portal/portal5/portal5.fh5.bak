$(function(){
    var angle = 0;
    var n = 0;
    $("#nav_img").rotate({
        bind: {
            click: function(){
                if(n==0){
                    $(this).rotate({ angle:0,animateTo:180,easing: $.easing.easeInOutExpo })
                    n= 1;
                }else{
                    $(this).rotate({ angle:0,animateTo:-180,easing: $.easing.easeInOutExpo })
                    n = 0;
                }
            }
        }
    });
});
