    setTimeout(function () {
      $("#main").addClass('pt-page-rotateSlideIn')
      $("#main").html(dbd);
    },400)
  }else{
    getOther(dbdK,tt,dbdcs);
  }
};
var getOther=function(dbdK,tt,dbdcs){
  //console.info(dbdcs);
  $.ajax({
      type: "get",
      async: true,
      dataType:"jsonp",
      url: "/db/jsonp/ssdb0/"+dbdK,
      success: function(response) {
        if(dbdK.indexOf("dashboard:")!="-1"){
          $("#dbdtitle").text(tt);
          var dbd='<iframe width="100%" height="100%" frameborder=0 src="../../dbd/dbd_view.fh5?key='+dbdK+'&dtitle='+tt+'&mh=portal2"></iframe>';
        //console.info(dbd);
        if($("#main").hasClass('pt-page-rotateSlideIn')){
          $("#main").removeClass('pt-page-rotateSlideIn');
        }

        setTimeout(function () {
          $("#main").addClass('pt-page-rotateSlideIn')
          $("#main").html(dbd);
        },400)
        }else if (dbdK.indexOf("am:")!="-1") {
          if (dbdcs) {
            $("#dbdtitle").text(tt);
            var dbd='<iframe width="100%" height="100%" frameborder=0 src="../../dbd/am_run.fh5?key='+dbdK+'&atitle='+tt+'&'+dbdcs+'&mh=portal2"></iframe>';
          }else{
            $("#dbdtitle").text(tt);
            var dbd='<iframe width="100%" height="100%" frameborder=0 src="../../dbd/am_run.fh5?key='+dbdK+'&atitle='+tt+'&mh=portal2"></iframe>';
          }
          if($("#main").hasClass('pt-page-rotateSlideIn')){
  					$("#main").removeClass('pt-page-rotateSlideIn');
  				}

          setTimeout(function () {
            $("#main").addClass('pt-page-rotateSlideIn')
            $("#main").html(dbd);
          },400)
        }
        else if (dbdK.indexOf("dashboard2:")!="-1") {
          $.ajax({
            type: "get",
            async: true,
            timeout : 1000*600,
            dataType:"jsonp",
            url: "/db/jsonp/ssdb0/"+dbdK,
            success:function(dataAll){
              var cs=dataAll[0].dcs;
              var oo=[];
              var cs_obj={};
              $.each(cs,function(name,value){
                var cs_connect=name+'='+value;
                cs_obj[name]=value;//讲原来的参数组装成对象；
                oo.push(cs_connect);
              });

              if(dbdcs){
                dbdcs=dbdcs.split("&");
                var dbdcs_obj={};
                for (var i = 0; i < dbdcs.length; i++) {
                  dbdcs_obj[dbdcs[i].split("=")[0]]=unescape(dbdcs[i].split("=")[1]);//讲导航的参数组装成对象；
                }
                for(var j in cs_obj){
                  var value=cs_obj[j];
                  for(var i in dbdcs_obj){
                    if(i==j){
                      var d=dbdcs_obj[i].replace(j,value);//将新参数和老参数进行匹配；
                      cs_obj[j]=d;
                    }
                  }
                }
                var params = $.param(cs_obj);
                cs= decodeURIComponent(params);
                //console.info(cs);
              }else{
                cs=oo.join("&");
              }
              $("#dbdtitle").text(tt);
              var dbd='<iframe width="100%" height="100%" frameborder=0 src="../../dbd/dbd2_view.fh5?key='+dbdK+'&d2title='+tt+'&'+cs+'&mh=portal2"></iframe>';
              if($("#main").hasClass('pt-page-rotateSlideIn')){
      					$("#main").removeClass('pt-page-rotateSlideIn');
      				}

              setTimeout(function () {
                $("#main").addClass('pt-page-rotateSlideIn')
                $("#main").html(dbd);
              },400)
            }
          });
        }
        else if (dbdK.indexOf("dp:")!="-1") {
          $.ajax({
            type: "get",
            async: true,
            timeout : 1000*600,
            dataType:"jsonp",
            url: "/db/jsonp/ssdb0/"+dbdK,
            success:function(dataAll){
              var cs=dataAll[0].mcs;
             //console.info(cs);
              var oo=[];
              $.each(cs,function(name,value){
                var cs_connect=name+'='+value;
                oo.push(cs_connect);

              });
              cs=oo.join("&");
             // console.info(cs);
             $("#dbdtitle").text(tt);
              var dbd='<iframe width="100%" height="100%" frameborder=0 src="../../dbd/more_view.fh5?key='+dbdK+'&mtitle='+tt+'&'+cs+'&mh=portal2"></iframe>';
              if($("#main").hasClass('pt-page-rotateSlideIn')){
      					$("#main").removeClass('pt-page-rotateSlideIn');
      				}

              setTimeout(function () {
                $("#main").addClass('pt-page-rotateSlideIn')
                $("#main").html(dbd);
              },400)
            }
          });
        }
      }
    });
};
