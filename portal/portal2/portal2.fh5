var getAjax=function(dbdK,tt,dbdcs){
  //console.info(dbdK);
  $("#big_box").fadeToggle();
  if (dbdK.indexOf("http")=="0") {
    var dbd='<iframe width="100%" height="100%" frameborder=0 src="'+dbdK+'?d2title='+tt+'"></iframe>';
    //console.info(dbd);
