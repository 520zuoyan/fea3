function GetRequest() {
    var url = location.search; //获取url中"?"符后的字串
    var a=decodeURI(url);
    if (a.indexOf("?") != -1) {
        var str = a.substr(1);
        strs = str.split("&");
        for(var i = 0; i < strs.length; i ++) {
            theRequest[strs[i].split("=")[0]]=unescape(strs[i].split("=")[1]);
        }
    }

     key=theRequest.key;
     ntitle=theRequest.ntitle;
     plogo=theRequest.logo;
    // console.info(plogo);
    return theRequest;
}
